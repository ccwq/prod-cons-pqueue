    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.10",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
